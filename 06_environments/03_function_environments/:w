  b <- function() {
    cat('Called from b: ')
    l <- c(b = environment(),  defined_in =  parent.env(environment()), called_from = parent.frame())
    print(l)
    print(identical(parent.frame(), parent.env(environment())))
    c()
  }
  c <- function() {
    cat('Called from c: ')
#    c(c = environment(), defined_in =  parent.env(environment()), called_from = parent.frame())
    identical(parent.frame(), parent.env(environment()))
  }
  b()

